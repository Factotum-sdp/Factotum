{
  "rules": {
    "Sheet-shift": {
      ".read": true,
      ".write": "root.child('profile-dispatch').child(auth.token.email.replace('.', ',')).child('role').val() === 'BOSS'"
    },
    "profile-dispatch": {
      ".read": true,
      ".write": true
    },
    "contacts": {
      ".read": true,
      ".write": true
    }
  }
}